<h2> My Rooms </h2>

<ul>
  <% roomsAndStuff.forEach((oneRoom)=>{ %>
    <li>
      <h3> <%= oneRoom.name %> </h3>
      <img src='<%= oneRoom.photoUrl %>'>
      <p> Owner: <i> <%= oneRoom.owner %> </i> </p>

    <%  if (oneRoom.hasGhosts ){ %>
        <p> Ghosts Friendly and has Monsters under the bed! Yay!</p>
    <%  } else { %>
        <p> Ghost Free and no monsters :( </p>
    <%  } %>
    </li>
  <% }); %>
</ul>
