<h2 class="container text-center"> My Rooms </h2>

<ul class="container text-center">
  <% roomsAndStuff.forEach((oneRoom)=>{ %>
    <li>
      <h3> <%= oneRoom.name %> </h3>
      <img src='<%= oneRoom.photoUrl %>'>
      <p> Owner: <i> <%= oneRoom.owner %> </i> </p>

    <%  if (oneRoom.hasPets ){ %>
        <p> Pet Friendly, yay!</p>
    <%  } else { %>
        <p> No pets please! </p>
    <%  } %>
    </li>
  <% }); %>
</ul>
